{
  "name": "discip-yourself",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "cap:open:ios": "npx cap open ios",
    "cap:sync": "npm run build && npx cap sync",
    "dev": "vite --port 5173 --strictPort --host 127.0.0.1",
    "dev:lan": "vite --port 5173 --strictPort --host 0.0.0.0",
    "dev:5174": "vite --port 5174 --strictPort --host 127.0.0.1",
    "dev:lan:5174": "vite --port 5174 --strictPort --host 0.0.0.0",
    "build": "vite build",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "preview": "vite preview --port 4174 --strictPort --host 127.0.0.1",
    "preview:lan": "vite preview --port 4174 --strictPort --host 0.0.0.0",
    "test": "vitest run src",
    "test:e2e": "playwright test",
    "check:da": "node scripts/check-da.mjs",
    "check": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@capacitor/cli": "^6.1.0",
    "@capacitor/core": "^6.1.0",
    "@capacitor/ios": "^6.1.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.4",
    "vitest": "^1.6.0"
  }
}
